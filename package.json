{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel openSource lint:watch test:watch start-mockapi",
    "openSource": "babel-node buildScripts/srcServer.js",
    "share": "lt --port 3000",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generateMockData": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generateMockData",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "author": "Cory House",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "faker": "^4.1.0",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^15.2.1",
    "json-schema-faker": "^0.5.0-rc23",
    "json-server": "^0.15.1",
    "localtunnel": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^7.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  }
}
